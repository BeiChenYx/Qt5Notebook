/* 通用QToolButton */
QPushButton,
QToolButton{
	height: 15px;
	color: rgb(66, 64, 67);
	padding-left: 15px;
	padding-right: 15px;
	border: 2px solid rgb(201, 213, 239);
	border-radius: 5px;
	background-color: rgb(242, 245, 254);
}
QPushButton:checked,
QToolButton:checked{
	color: rgb(238, 242, 245);
	border: 2px solid rgb(75, 144, 222);
	background-color: rgb(75, 144, 222);
}
QPushButton:hover,
QToolButton:hover{
	color: rgb(238, 242, 245);
	border: 2px solid rgb(75, 144, 222);
	background-color: rgb(75, 144, 222);
}
QPushButton:!enabled,
QToolButton:!enabled{
	background-color: rgb(230, 230, 230);
}

QHeaderView{
    background: transparent;
}

QTableView{
    /*设计师里面要勾选alternateRowColors选项*/
    alternate-background-color: rgb(242,242,242);
    background-color: rgb(255,255,255);
    selection-background-color: #0078D7;
    selection-color: #FAFAFA;
    gridline-color: rgb(230, 230, 230);
    border-top: none;
    border-right: 1px solid rgb(230, 230, 230);
    border-left: 1px solid rgb(230, 230, 230);
    border-bottom: 1px solid rgb(230, 230, 230);
}

QTableView QHeaderView::section:horizontal{
    background-color:qlineargradient(x1:0,y1:1,x2:0,y2:0,stop:0 rgba(17,127,225, 1),stop:1 rgba(48,150,248, 0.5));
    border: none;
    padding-left:5px;
    padding-right:5px;
	color: rgb(255, 255, 255);
}
QTableView QHeaderView::section:vertical {
    background-color:rgb(230, 230, 230);
    border: none;
    padding-left:5px;
    padding-right:5px;
	color: rgb(66, 64, 67);
}
QTableView QHeaderView::section:!last{
    border-right: 1px solid rgb(230, 230, 230);
}
QScrollBar::handle{
    background-color: rgb(230,230,230);
    margin: 3px;
    border-radius: 4px;
}
QScrollBar{
    background-color: rgb(255,255,255);
    border-style: solid;
    border-radius: 4px;
}
QScrollBar::add-page, QScrollBar::sub-page{
    background: transparent;
}
QScrollBar::add-line, QScrollBar::sub-line{
    background: transparent;
}
QLineEdit{
    border-radius: 2px;
    border: 1px solid rgb(155, 155, 155);
	color: #000000;
    background-color: rgb(255, 255, 255);
    selection-background-color: #0078D7;
}
QLineEdit:enabled{
    background-color: rgb(255, 255, 255);
}
QLineEdit:!enabled{
    background-color: rgb(230, 230, 230);
}
QLineEdit:enabled:hover,
QLineEdit:enabled:focus{
    background-color: rgb(255, 255, 255);
    border: 1px solid #3190EE;
}

QProgressBar {
    border: 2px solid #237fca;
    text-align: right;
	margin-right: 25px;
}
QProgressBar::chunk {
    background-color: #237fca;
    width: 10px;
    margin: 0.5px;
}
QComboBox {
	height: 20px;
    color: rgb(66, 64, 67);
	border: 1px solid rgb(155, 155, 155);
	border-radius: 2px;
    background-color: rgb(255, 255, 255);
    selection-background-color: #0078D7;
}
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    background-color: rgb(255, 255, 255);
    width: 14px;
    height: 20px;
}
QComboBox::down-arrow {
	image: url(:/images/down.png);
    width: 12px;
    height: 12px;
}
QComboBox::down-arrow:hover {
    width: 14px;
    height: 14px;
}
QComboBox QAbstractItemView {
    border: none;
    selection-background-color: #0078D7;
}
QSpinBox {
	height: 20px;
    color: rgb(66, 64, 67);
	border: 1px solid rgb(155, 155, 155);
	border-radius: 2px;
    background-color: rgb(255, 255, 255);
    selection-background-color: #0078D7;
}
QSpinBox:hover {
    border: 1px solid #3190EE;
}
QSpinBox::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 12px;
    height: 12px;
    border-width: 1px;
}

QSpinBox::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;
    width: 12px;
    height: 12px;
    border-width: 1px;
    border-top-width: 1px;
}
QSpinBox::up-arrow {
	image: url(:/images/up.png);
    width: 12px;
    height: 12px;
}
QSpinBox::up-arrow:hover {
    width: 14px;
    height: 14px;
}
QSpinBox::down-arrow {
	image: url(:/images/down.png);
    width: 12px;
    height: 12px;
}
QSpinBox::down-arrow:hover {
    width: 14px;
    height: 14px;
}
QTextBrowser{
